{
  "name": "@actually_connor/uuid",
  "version": "0.3.10",
  "description": "A JavaScript library that provides a 'ramsey/uuid'-like interface for the uuid package",
  "keywords": [
    "uuid",
    "identifier",
    "guid",
    "rfc4122"
  ],
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "dist/index.js",
  "exports": {
    ".": {
      "node": {
        "module": "./dist/esm-node/index.js",
        "require": "./dist/index.js"
      },
      "default": "./dist/esm-browser/index.js"
    },
    "./package.json": "./package.json"
  },
  "module": "./dist/esm-node/index.js",
  "browser": {
    "./dist/esm-node/index.js": "./dist/esm-browser/index.js"
  },
  "scripts": {
    "test": "jest",
    "prepare": "husky install",
    "lint": "eslint --ext .ts src",
    "lint-fix": "eslint --ext .ts src --fix",
    "build": "./scripts/build.sh",
    "docs": "./scripts/docs.sh"
  },
  "author": {
    "name": "Connor Smyth",
    "email": "connorsmyth@rogers.com"
  },
  "license": "MIT",
  "dependencies": {
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "docdash": "^1.2.0",
    "eslint": "^8.19.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2"
  },
  "bugs": {
    "url": "https://github.com/ActuallyConnor/uuid/issues"
  },
  "homepage": "https://github.com/ActuallyConnor/uuid",
  "repository": {
    "type": "git",
    "url": "https://github.com/ActuallyConnor/uuid"
  }
}
